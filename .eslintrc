{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest/globals": true
	},
	"rules": {
		"semi": "off",
		"arrow-parent": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"operator-linebreak": "off",
		"no-tabs": "off",
		"no-mixed-spaces-and-tabs": "off",
		"max-len": ["error", {
			"code": 120,
			"tabWidth": 4
		}]
	},
	"extends": ["eslint:recommended", "google"],
	"plugins": ["jest"]
}